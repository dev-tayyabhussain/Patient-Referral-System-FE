{
  "version": 2,
  "name": "medinet-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://medinet-backend.onrender.com",
    "VITE_APP_NAME": "MediNet",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://medinet-backend.onrender.com",
      "VITE_APP_NAME": "MediNet",
      "VITE_APP_VERSION": "1.0.0",
      "VITE_NODE_ENV": "production"
    }
  }
}
